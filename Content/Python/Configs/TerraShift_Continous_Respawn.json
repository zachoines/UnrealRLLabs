        "StationaryPenalty_Drain": 0.01,
        "PotentialShaping_Gamma": 0.99,
        "PotentialShaping_Scale": 1.0,
        "VelAlign_Scale": 0.5,
        "VelAlign_Min": -50.0,
        "VelAlign_Max": 50.0,
        "DistImprove_Scale": 100.0,
        "DistImprove_Min": -1.0,
        "DistImprove_Max": 1.0,
        "ZAccel_Scale": 0.0001,
        "ZAccel_Min": 0.1,
        "ZAccel_Max": 2000.0,
        "EventReward_GoalReached": 10.0,
        "EventReward_OutOfBounds": -2.5,
        "TimeStepPenalty": 0.0
