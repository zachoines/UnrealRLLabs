  "StateRecorder": {
    "enabled": false,
    "video_settings": {
      "output_path": "Recordings\\TerraShift.mp4",
      "fps": 32,
      "dpi": 150
    },
    "auto_save_every": 512,
    "components_to_record": [
      {
        "enabled": true,
        "source_key": "central",
        "component_name": "height_map",
        "visualization_type": "3d_surface",
        "plot_title": "Height Map Visualization",
        "shape_for_plot": {
          "h": 64,
          "w": 64,
          "c": 1
        },
        "z_lim_3d": [
          -1,
          1
        ],
        "cmap_3d": "terrain"
      }
    ]
  },
  "agent": {
    "type": "MA_POCA",
    "params": {
      "enable_memory": true,
      "memory_type": "gru",
      "gru": {
        "input_size": 256,
        "hidden_size": 256,
        "num_layers": 1,
        "bias": true,
        "dropout": 0.1
      },
      "disagreement_rewards_disabled": {
        "enabled": false,
        "reward_coeff": 0.1,
        "loss_coeff": 0.5,
        "normalize_reward": true,
        "ensemble_size": 8,
        "network": {
          "hidden_size": 64
        }
      },
      "rnd_reward_disabled": {
        "enabled": false,
        "reward_coeff": 0.01,
        "loss_coeff": 1.0,
        "update_proportion": 0.25,
        "normalize_reward": false,
        "network": {
          "output_size": 128,
          "hidden_size": 256
        }
      },
      "rewards_normalizer_disabled": {
        "epsilon": 1e-05,
        "warmup_steps": 0
      },
      "enable_popart": false,
      "popart_beta": 0.995,
      "popart_epsilon": 0.0001,
      "learning_rate": 7.5e-05,
      "lr_policy": 7.5e-05,
      "lr_trunk": 7.5e-05,
      "lr_value_path": 7.5e-05,
      "lr_baseline_path": 7.5e-05,
      "lr_value_quantile": 7.5e-05,
      "lr_value_fraction": 7.5e-05,
      "lr_value_iqn": 7.5e-05,
      "lr_baseline_iqn": 7.5e-05,
      "lr_rnd_predictor": 7.5e-05,
      "lr_disagreement": 7.5e-05,
      "optimizer_betas": {
        "default": [
          0.9,
          0.999
        ]
      },
      "weight_decay": {
        "default": 0.0
      },
      "gamma": 0.95,
      "lmbda": 0.92,
      "value_loss_coeff": 1.0,
      "baseline_loss_coeff": 0.5,
      "normalize_advantages": true,
      "max_grad_norm": 1.0,
      "ppo_clip_range": 0.2,
      "value_clip_range": 1000.0,
      "entropy_coeff": 0.01,
      "schedulers": {
        "lr_trunk": {
          "start_factor": 1.0,
          "end_factor": 0.1,
          "total_iters": 12000
        },
        "lr_policy": {
          "start_factor": 1.0,
          "end_factor": 0.1,
          "total_iters": 12000
        },
        "lr_value_path": {
          "start_factor": 1.0,
          "end_factor": 0.1,
          "total_iters": 20000
        },
        "lr_value_quantile": {
          "start_factor": 1.0,
          "end_factor": 0.3,
          "total_iters": 30000
        },
        "lr_value_fraction": {
          "start_factor": 1.0,
          "end_factor": 0.3,
          "total_iters": 30000
        },
        "lr_baseline_path": {
          "start_factor": 1.0,
          "end_factor": 0.1,
          "total_iters": 20000
        },
        "lr_rnd_predictor": {
          "start_factor": 1.0,
          "end_factor": 0.1,
          "total_iters": 20000
        },
        "lr_disagreement": {
          "start_factor": 1.0,
          "end_factor": 0.1,
          "total_iters": 20000
        },
        "entropy_coeff": {
          "start_value": 0.01,
          "end_value": 0.001,
          "total_iters": 20000
        },
        "policy_clip": {
          "start_value": 0.2,
          "end_value": 0.1,
          "total_iters": 20000
        },
        "value_clip_disabled": {
          "start_value": 1.0,
          "end_value": 0.1,
          "total_iters": 10000
        },
        "max_grad_norm": {
          "start_value": 3.0,
          "end_value": 1.0,
          "total_iters": 20000
        },
        "fqf_temperature": {
          "start_value": 3.0,
          "end_value": 0.5,
          "total_iters": 30000
        },
        "fqf_prior_blend_alpha": {
          "start_value": 0.2,
          "end_value": 0.0,
          "total_iters": 25000
        },
        "fraction_entropy_coeff": {
          "start_value": 0.02,
          "end_value": 0.001,
          "total_iters": 30000
        },
        "fraction_loss_coeff": {
          "start_value": 0.05,
          "end_value": 0.2,
          "total_iters": 20000
        }
      },
      "networks": {
        "MultiAgentEmbeddingNetwork": {
          "cross_attention_feature_extractor": {
            "agent_obs_size": 9,
            "num_agents": 16,
            "embed_dim": 256,
            "num_heads": 8,
            "transformer_layers": 2,
            "dropout_rate": 0.1,
            "use_agent_id": true,
            "ff_hidden_factor": 2,
            "id_num_freqs": 8,
            "conv_init_scale": 1.0,
            "linear_init_scale": 1.0,
            "fusion_type": "parallel",
            "central_processing_configs": [
              {
                "name": "height_map",
                "enabled": true,
                "type": "cnn",
                "cnn_stem_channels": [
                  24,
                  48,
                  96
                ],
                "cnn_stem_kernels": [
                  5,
                  3,
                  3
                ],
                "cnn_stem_strides": [
                  2,
                  2,
                  2
                ],
                "cnn_stem_group_norms": [
                  6,
                  12,
                  24
                ],
                "target_pool_scale": 4,
                "add_spatial_coords_to_cnn_input": true,
                "swin_stages": [
                  {
                    "window_size": 4,
                    "num_heads": 4,
                    "merge": false
                  },
                  {
                    "window_size": 4,
                    "num_heads": 8,
                    "merge": true
                  },
                  {
                    "window_size": 2,
                    "num_heads": 8,
                    "merge": false
                  }
                ],
                "adaptive_tokenizer": {
                  "base_tokens": 12,
                  "min_tokens": 8,
                  "max_tokens": 20,
                  "importance_hidden": 96
                }
              },
              {
                "name": "gridobject_sequence",
                "enabled": true,
                "type": "linear_sequence",
                "linear_stem_hidden_sizes": [
                  128,
                  256
                ],
                "add_positional_encoding": true
              }
            ]
          },
          "obs_encoder": {
            "state_dim": 256,
            "hidden_size": 256,
            "output_size": 256,
            "dropout_rate": 0.1,
            "activation": true,
            "layer_norm": true
          },
          "obs_actions_encoder": {
            "state_dim": 256,
            "action_dim": 6,
            "hidden_size": 256,
            "output_size": 256,
            "dropout_rate": 0.1,
            "activation": true,
            "layer_norm": true
          }
        },
        "policy_network": {
          "type": "beta",
          "in_features": 256,
          "hidden_size": 256,
          "param_init_bias": 0.0,
          "min_concentration": 1.01,
          "linear_init_scale": 0.1,
          "dropout_rate": 0.1
        },
        "critic_network": {
          "baseline_head": {
            "in_features": 256,
            "hidden_size": 256,
            "dropout_rate": 0.0,
            "output_layer_init_gain": 1.0
          },
          "value_head": {
            "in_features": 256,
            "hidden_size": 256,
            "dropout_rate": 0.0,
            "output_layer_init_gain": 1.0
          },
          "value_attention": {
            "embed_dim": 256,
            "num_heads": 8,
            "dropout": 0.1,
            "self_attention": true
          },
          "baseline_attention": {
            "embed_dim": 256,
            "num_heads": 8,
            "dropout": 0.1,
            "self_attention": true
          },
          "distributional_network_params_disabled": {
            "iqn_network_disabled": {
              "feature_dim": 256,
              "hidden_size": 256,
              "quantile_embedding_dim": 64,
              "num_quantiles": 32,
              "use_dueling": true,
              "linear_init_scale": 1.0,
              "dropout_rate": 0.1,
              "iqn_kappa": 1.0
            },
            "fqf_network_disabled": {
              "feature_dim": 256,
              "hidden_size": 256,
              "quantile_embedding_dim": 64,
              "num_quantiles": 32,
              "use_dueling": true,
              "linear_init_scale": 1.0,
              "dropout_rate": 0.1,
              "iqn_kappa": 1.0,
              "fraction_net_hidden": 128,
              "fraction_loss_coeff": 0.05,
              "fraction_entropy_coeff": 0.02,
              "softmax_temperature": 3.0,
              "prior_blend_alpha": 0.2,
              "exploration_noise_scale": 0.02,
              "logit_clamp_epsilon": 1e-08,
              "monotonicity_epsilon": 1e-06
            }
          }
        }
      }
    }
  },
  "train": {
    "sequence_length": 32,
    "pad_trajectories": true,
    "batch_size": 512,
    "buffer_size": 512,
    "num_environments": 16,
    "epochs": 3,
    "mini_batch_size": 16,
    "ActionRepeat": 3,
    "saveFrequency": 9,
    "states_normalizer_disabled": {
      "epsilon": 1e-08,
      "warmup_steps": 0
    }
  },
  "test": {
    "enabled": false,
    "frequency": 1,
    "steps": 128
  }
}
