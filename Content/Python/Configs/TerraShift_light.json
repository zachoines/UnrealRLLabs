{
  "environment": {
    "name": "TerraShift_8A_32x32_Test",
    "id": "TerraShift_8A_32x32_Test_$(date +%Y%m%d_%H%M%S)",
    "params": {
      "PlatformSize": 1.0,
      "MaxColumnHeight": 6,
      "ObjectMass": 1.0,
      "GridSize": 16,
      "MaxSteps": 256,
      "MaxAgents": 4,
      "TerraShiftEnvironment": {
        "bUseVelAlignment": false,
        "bUseXYDistanceImprovement": true,
        "bUseZAccelerationPenalty": false,
        "bUsePotentialShaping": false,
        "bUsePositivePotentialShaping": false,
        "bUseExponentialPotential": false,
        "PositivePotential_Power": 1.0,
        "PositivePotential_Sigma": 0.4,
        "bUseDistanceBasedReward": false,
        "bDisableEventRewards": false,
        "bUseStationaryPenalty": false,
        "StationaryPenalty_MinSpeed": 10.0,
        "StationaryPenalty_Drain": 0.0001,
        "StationaryPenalty_MinConsecutiveFrames": 1,
        "PotentialShaping_Gamma": 0.995,
        "PotentialShaping_Scale": 0.6,
        "VelAlign_Scale": 0.0,
        "VelAlign_Min": -25.0,
        "VelAlign_Max": 25.0,
        "DistImprove_Scale": 1.0,
        "DistImprove_Min": -1.0,
        "DistImprove_Max": 1.0,
        "ZAccel_Scale": 0.0,
        "ZAccel_Min": 0.1,
        "ZAccel_Max": 2000.0,
        "EventReward_GoalReached": 1.0,
        "EventReward_OutOfBounds": 0.25,
        "TimeStepPenalty": 0.0005
      },
      "GoalManager": {
        "DefaultRadius": 1.0
      },
      "MultiAgentGaussianWaveHeightMap": {
        "NumAgents": 4,
        "GridSize": 16,
        "MinHeight": -6,
        "MaxHeight": 6,
        "NumActions": 6,
        "bUseActionDelta": true,
        "bAccumulatedWave": false,
        "AccumulatedWaveFadeGamma": 0.9,
        "bWrapAroundEdges": true,
        "VelMinMax": [
          -1.0,
          1.0
        ],
        "AngVelRange": [
          -0.785,
          0.785
        ],
        "AmpMinMax": [
          -8.0,
          8.0
        ],
        "SigMinMax": [
          0.0,
          8.0
        ],
        "DeltaVelScale": 1,
        "DeltaAngVelScale": 1,
        "DeltaAmpScale": 0.5,
        "DeltaSigmaScale": 0.5
      },
      "StateManager": {
        "MaxGridObjects": 2,
        "MarginXY": -1.0,
        "MinZ": 0.0,
        "MaxZ": 50.0,
        "SpawnPaddingZ": 15.0,
        "SpawnSeparationRadius": 8.0,
        "ObjectMass": 1.0,
        "MaxColumnHeight": 6,
        "BaseRespawnDelay": 0.1,
        "OverheadCameraDistance_disabled": 1500.0,
        "OverheadCameraFOV_disabled": 90.0,
        "OverheadCameraResX_disabled": 64,
        "OverheadCameraResY_disabled": 64,
        "bUseRandomGoals": true,
        "bRespawnOnGoal": false,
        "bRespawnOnOOB": false,
        "bTerminateOnAllGoalsReached": true,
        "bTerminateOnAllObjectsOutOfBounds": true,
        "bTerminateOnMaxSteps": false,
        "bRemoveObjectsOnGoal": true,
        "bSuppressPerStepGoalReward": true,
        "GoalRadius": 8.0,
        "ObjectRadius": 8.0,
        "GoalCollectRadius": 8.0,
        "GoalColors": [
          [
            1.0,
            0.0,
            1.0
          ],
          [
            1.0,
            1.0,
            0.0
          ]
        ],
        "GridObjectColors": [
          [
            1.0,
            0.0,
            1.0
          ],
          [
            1.0,
            1.0,
            0.0
          ]
        ],
        "bIncludeHeightMapInState": true,
        "StateHeightMapResolutionH": 32,
        "StateHeightMapResolutionW": 32,
        "bIncludeOverheadImageInState": false,
        "StateOverheadImageResX": 64,
        "StateOverheadImageResY": 64,
        "bIncludeGridObjectSequenceInState": true,
        "MaxGridObjectsForState": 2,
        "GridObjectFeatureSize": 9,
        "bRestrictColumnPhysicsToRadius": true,
        "ColumnCollisionRadiusCells": 2,
        "bEnabledShaderBasedHeightmap": true,
        "bRestrictColumnMovementToRadius": true
      }
    },
    "shape": {
      "state": {
        "central": [
          {
            "name": "height_map",
            "enabled": true,
            "type": "matrix2d",
            "shape": {
              "h": 32,
              "w": 32,
              "c": 1
            }
          },
          {
            "name": "greyscale_image",
            "enabled": false,
            "type": "matrix2d",
            "shape": {
              "h": 64,
              "w": 64,
              "c": 1
            }
          },
          {
            "name": "gridobject_sequence",
            "enabled": true,
            "type": "sequence",
            "shape": {
              "max_length": 2,
              "feature_dim": 9
            }
          }
        ],
        "agent": {
          "obs_size": 9,
          "min": 4,
          "max": 4
        }
      },
      "action": {
        "agent": {
          "continuous": [
            {
              "min": -1,
              "max": 1
            },
            {
              "min": -1,
              "max": 1
            },
            {
              "min": -1,
              "max": 1
            },
            {
              "min": -1,
              "max": 1
            },
            {
              "min": -1,
              "max": 1
            },
            {
              "min": -1,
              "max": 1
            }
          ]
        }
      }
    }
  },
  "StateRecorder": {
    "enabled": false,
    "video_settings": {
      "output_path": "Recordings\\TerraShift.mp4",
      "fps": 32,
      "dpi": 150
    },
    "auto_save_every": 256,
    "components_to_record": [
      {
        "enabled": true,
        "source_key": "central",
        "component_name": "height_map",
        "visualization_type": "3d_surface",
        "plot_title": "Height Map Visualization",
        "shape_for_plot": {
          "h": 32,
          "w": 32,
          "c": 1
        },
        "z_lim_3d": [
          -1,
          1
        ],
        "cmap_3d": "terrain"
      }
    ]
  },
  "agent": {
    "type": "MA_POCA",
    "params": {
      "enable_memory": true,
      "memory_type": "gru",
      "gru": {
        "input_size": 128,
        "hidden_size": 128,
        "num_layers": 1,
        "bias": true,
        "dropout": 0.05
      },
      "rnd_rewards": {
        "reward_coeff": 0.01,
        "loss_coeff": 1.0,
        "update_proportion": 0.5,
        "normalize_reward": true,
        "network": {
          "output_size": 128,
          "hidden_size": 256
        }
      },
      "rewards_normalizer": {
        "epsilon": 1e-05,
        "warmup_steps": 0
      },
      "enable_popart": false,
      "popart_beta": 0.995,
      "popart_epsilon": 0.0001,
      "learning_rate": 0.00035,
      "lr_policy": 0.00035,
      "lr_trunk": 0.00035,
      "lr_value_path": 0.00035,
      "lr_baseline_path": 0.00035,
      "lr_value_quantile": 0.0004,
      "lr_value_fraction": 7e-05,
      "lr_value_iqn": 0.0004,
      "lr_baseline_iqn": 0.0004,
      "lr_rnd_predictor": 0.0004,
      "lr_disagreement": 0.0003,
      "optimizer_betas": {
        "default": [
          0.9,
          0.999
        ]
      },
      "weight_decay": {
        "default": 0.0
      },
      "gamma": 0.97,
      "lmbda": 0.95,
      "value_loss_coeff": 0.6,
      "baseline_loss_coeff": 0.3,
      "normalize_advantages": true,
      "max_grad_norm": 0.7,
      "ppo_clip_range": 0.18,
      "value_clip_range": 0.5,
      "entropy_coeff": 0.015,
      "schedulers": {
        "lr_trunk": {
          "start_factor": 1.0,
          "end_factor": 0.5,
          "total_iters": 1000000
        },
        "lr_policy": {
          "start_factor": 1.0,
          "end_factor": 0.5,
          "total_iters": 1000000
        },
        "lr_value_path": {
          "start_factor": 1.0,
          "end_factor": 0.5,
          "total_iters": 1000000
        },
        "lr_value_quantile": {
          "start_factor": 1.0,
          "end_factor": 0.3333,
          "total_iters": 1000000
        },
        "lr_value_fraction": {
          "start_factor": 1.0,
          "end_factor": 0.5,
          "total_iters": 1000000
        },
        "lr_baseline_path": {
          "start_factor": 1.0,
          "end_factor": 0.5,
          "total_iters": 1000000
        },
        "lr_rnd_predictor": {
          "start_factor": 1.0,
          "end_factor": 0.5,
          "total_iters": 1000000
        },
        "lr_disagreement": {
          "start_factor": 1.0,
          "end_factor": 0.5,
          "total_iters": 1000000
        },
        "entropy_coeff": {
          "start_value": 0.001,
          "end_value": 0.0005,
          "total_iters": 1000000
        },
        "policy_clip": {
          "start_value": 0.2,
          "end_value": 0.1,
          "total_iters": 1000000
        },
        "value_clip": {
          "start_value": 0.5,
          "end_value": 0.1,
          "total_iters": 1000000
        },
        "max_grad_norm": {
          "start_value": 3.0,
          "end_value": 1.0,
          "total_iters": 1000000
        },
        "fqf_temperature": {
          "start_value": 6.0,
          "end_value": 4.0,
          "total_iters": 1000000
        },
        "fqf_prior_blend_alpha": {
          "start_value": 0.3,
          "end_value": 0.15,
          "total_iters": 1000000
        },
        "fraction_entropy_coeff": {
          "start_value": 0.08,
          "end_value": 0.02,
          "total_iters": 1000000
        },
        "fraction_loss_coeff": {
          "start_value": 0.03,
          "end_value": 0.06,
          "total_iters": 1000000
        },
        "rnd_reward_coeff": {
          "start_value": 0.005,
          "end_value": 0.0,
          "total_iters": 1000000
        }
      },
      "networks": {
        "MultiAgentEmbeddingNetwork": {
          "cross_attention_feature_extractor": {
            "agent_obs_size": 9,
            "num_agents": 4,
            "embed_dim": 128,
            "num_heads": 4,
            "transformer_layers": 1,
            "dropout_rate": 0.05,
            "use_agent_id": true,
            "ff_hidden_factor": 2,
            "id_num_freqs": 8,
            "conv_init_scale": 1.0,
            "linear_init_scale": 1.0,
            "fusion_type": "parallel",
            "central_processing_configs": [
              {
                "name": "height_map",
                "enabled": true,
                "type": "cnn",
                "cnn_stem_channels": [
                  16,
                  32
                ],
                "cnn_stem_kernels": [
                  3,
                  3
                ],
                "cnn_stem_strides": [
                  2,
                  2
                ],
                "cnn_stem_group_norms": [
                  4,
                  8
                ],
                "target_pool_scale": 2,
                "add_spatial_coords_to_cnn_input": true,
                "swin_stages": [
                  {
                    "window_size": 2,
                    "num_heads": 4,
                    "merge": false
                  }
                ],
                "adaptive_tokenizer": {
                  "base_tokens": 6,
                  "min_tokens": 4,
                  "max_tokens": 10,
                  "importance_hidden": 64
                }
              },
              {
                "name": "gridobject_sequence",
                "enabled": true,
                "type": "linear_sequence",
                "linear_stem_hidden_sizes": [
                  96,
                  128
                ],
                "add_positional_encoding": true
              }
            ]
          },
          "obs_encoder": {
            "state_dim": 128,
            "hidden_size": 128,
            "output_size": 128,
            "dropout_rate": 0.05,
            "activation": true,
            "layer_norm": true
          },
          "obs_actions_encoder": {
            "state_dim": 128,
            "action_dim": 6,
            "hidden_size": 128,
            "output_size": 128,
            "dropout_rate": 0.05,
            "activation": true,
            "layer_norm": true
          }
        },
        "policy_network": {
          "type": "beta",
          "in_features": 128,
          "hidden_size": 128,
          "param_init_bias": 0.0,
          "min_concentration": 1.01,
          "linear_init_scale": 0.2,
          "dropout_rate": 0.05
        },
        "critic_network": {
          "baseline_head": {
            "in_features": 128,
            "hidden_size": 128,
            "dropout_rate": 0.0,
            "output_layer_init_gain": 1.0
          },
          "value_head": {
            "in_features": 128,
            "hidden_size": 128,
            "dropout_rate": 0.0,
            "output_layer_init_gain": 1.0
          },
          "value_attention": {
            "embed_dim": 128,
            "num_heads": 4,
            "dropout": 0.05,
            "self_attention": true
          },
          "baseline_attention": {
            "embed_dim": 128,
            "num_heads": 4,
            "dropout": 0.05,
            "self_attention": true
          },
          "distributional_network_params_disabled": {}
        }
      }
    }
  },
  "train": {
    "sequence_length": 16,
    "pad_trajectories": true,
    "batch_size": 256,
    "buffer_size": 256,
    "num_environments": 128,
    "epochs": 2,
    "mini_batch_size": 128,
    "ActionRepeat": 1,
    "saveFrequency": 40,
    "states_normalizer": {
      "epsilon": 1e-08,
      "warmup_steps": 0
    },
    "checkpoint": {
      "path": "checkpoints//test.pth",
      "restore_optimizers": false,
      "restore_schedulers": false
    }
  },
  "test": {
    "enabled": true,
    "eval_only": false,
    "frequency": 20,
    "steps": 256
  }
}