{
  "environment": {
    "name": "TerraShift_Easy_NoRespawn",
    "id": "TerraShift_Easy_NoRespawn_$(date +%Y%m%d_%H%M%S)",
    "params": {
      "PlatformSize": 1.0,
      "MaxColumnHeight": 4.0,
      "ObjectMass": 0.1,
      "GridSize": 8,
      "MaxSteps": 256,
      "MaxAgents": 4,
      "TerraShiftEnvironment": {
        "bUseVelAlignment": false,
        "bUseXYDistanceImprovement": true,
        "bUseZAccelerationPenalty": false,
        "bUsePotentialShaping": false,
        "bUseDistanceBasedReward": false,
        "bDisableEventRewards": false,
        "bUseStationaryPenalty": false,
        "StationaryPenalty_MinSpeed": 5.0,
        "StationaryPenalty_Drain": 0.005,
        "PotentialShaping_Gamma": 0.99,
        "PotentialShaping_Scale": 1.0,
        "VelAlign_Scale": 0.25,
        "VelAlign_Min": -50.0,
        "VelAlign_Max": 50.0,
        "DistImprove_Scale": 100.0,
        "DistImprove_Min": -1.0,
        "DistImprove_Max": 1.0,
        "ZAccel_Scale": 0.0001,
        "ZAccel_Min": 0.1,
        "ZAccel_Max": 2000.0,
        "EventReward_GoalReached": 10.0,
        "EventReward_OutOfBounds": -10.0,
        "TimeStepPenalty": 0.0
      },
      "GoalManager": {
        "DefaultRadius": 1.0
      },
      "MultiAgentGaussianWaveHeightMap": {
        "NumAgents": 4,
        "GridSize": 8,
        "MinHeight": -4.0,
        "MaxHeight": 4.0,
        "NumActions": 6,
        "bUseActionDelta": true,
        "bAccumulatedWave": false,
        "AccumulatedWaveFadeGamma": 0.95,
        "bWrapAroundEdges": true,
        "VelMinMax": [-2, 2],
        "AngVelRange": [-0.785, 0.785],
        "AmpMinMax": [-4.0, 4.0],
        "SigMinMax": [0.1, 2.0],
        "DeltaVelScale": 2.0,
        "DeltaAngVelScale": 0.785,
        "DeltaAmpScale": 4.0,
        "DeltaSigmaScale": 1.0
      },
      "StateManager": {
        "MaxGridObjects": 2,
        "MarginXY": -1.0,
        "MinZ": 0.0,
        "MaxZ": 60.0,
        "SpawnPaddingZ": 10.0,
        "SpawnSeparationRadius": 8.0,
        "ObjectMass": 0.1,
        "MaxColumnHeight": 4.0,
        "BaseRespawnDelay": 0.1,
        "bEnableColumnCollisionOptimization": true,
        "ColumnCollisionRadiusCells": 2,
        "OverheadCameraDistance_disabled": 1500.0,
        "OverheadCameraFOV_disabled": 90.0,
        "OverheadCameraResX_disabled": 64,
        "OverheadCameraResY_disabled": 64,
        "bUseRandomGoals": true,
        "bRespawnOnGoal": false,
        "bRespawnOnOOB": false,
        "bTerminateOnAllGoalsReached": true,
        "bTerminateOnMaxSteps": false,
        "bRemoveObjectsOnGoal": true,
        "GoalRadius": 8.0,
        "ObjectRadius": 7.0,
        "GoalCollectRadius": 8.0,
        "GoalColors": [
          [1.0, 0.0, 1.0],
          [1.0, 1.0, 0.0]
        ],
        "GridObjectColors": [
          [1.0, 0.0, 1.0],
          [1.0, 1.0, 0.0]
        ],
        "GridSize": 8,
        "bIncludeHeightMapInState": true,
        "bIncludeOverheadImageInState": false,
        "StateHeightMapResolutionH": 32,
        "StateHeightMapResolutionW": 32,
        "bIncludeGridObjectSequenceInState": true,
        "MaxGridObjectsForState": 2,
        "GridObjectFeatureSize": 9
      }
    },
    "shape": {
      "state": {
        "central": [
          {
            "name": "height_map",
            "enabled": true,
            "type": "matrix2d",
            "shape": {"h": 32, "w": 32, "c": 1}
          },
          {
            "name": "gridobject_sequence",
            "enabled": true,
            "type": "sequence",
            "shape": {"max_length": 2, "feature_dim": 9}
          }
        ],
        "agent": {
          "obs_size": 9,
          "min": 4,
          "max": 4
        }
      },
      "action": {
        "agent": {
          "continuous": [
            {"min": -1.0, "max": 1.0},
            {"min": -1.0, "max": 1.0},
            {"min": -1.0, "max": 1.0},
            {"min": -1.0, "max": 1.0},
            {"min": -1.0, "max": 1.0},
            {"min": -1.0, "max": 1.0}
          ]
        }
      }
    }
  },
  "StateRecorder": {
    "enabled": true,
    "video_settings": {
      "output_path": "Recordings\\TerraShift_Easy_NoRespawn.mp4",
      "fps": 24,
      "dpi": 120
    },
    "auto_save_every": 256,
    "components_to_record": [
      {
        "enabled": true,
        "source_key": "central",
        "component_name": "height_map",
        "visualization_type": "3d_surface",
        "plot_title": "Height Map (32x32)",
        "shape_for_plot": { "h": 32, "w": 32, "c": 1 },
        "z_lim_3d": [-1, 1],
        "cmap_3d": "terrain"
      }
    ]
  },
  "agent": {
    "type": "MA_POCA",
    "params": {
      "enable_memory": true,
      "memory_type": "gru",
      "gru": {
        "input_size": 128,
        "hidden_size": 128,
        "num_layers": 1,
        "bias": true,
        "dropout": 0.1
      },
      "disagreement_rewards_disabled": {
        "enabled": false,
        "reward_coeff": 0.1,
        "loss_coeff": 0.5,
        "normalize_reward": true,
        "ensemble_size": 4,
        "network": { "hidden_size": 64 }
      },
      "rnd_reward_disabled": {
        "enabled": false,
        "reward_coeff": 0.01,
        "loss_coeff": 1.0,
        "update_proportion": 0.25,
        "normalize_reward": false,
        "network": { "output_size": 64, "hidden_size": 128 }
      },
      "rewards_normalizer_disabled": { "epsilon": 1e-05, "warmup_steps": 0 },
      "enable_popart": false,
      "learning_rate": 0.00025,
      "lr_policy": 0.00025,
      "lr_trunk": 0.00025,
      "lr_value_path": 0.00025,
      "lr_baseline_path": 0.00025,
      "optimizer_betas": {"default": [0.9, 0.999]},
      "weight_decay": {"default": 0.0},
      "gamma": 0.97,
      "lmbda": 0.9,
      "value_loss_coeff": 0.5,
      "baseline_loss_coeff": 0.5,
      "normalize_advantages": true,
      "max_grad_norm": 0.8,
      "ppo_clip_range": 0.2,
      "value_clip_range": 1.0,
      "entropy_coeff": 0.01,
      "schedulers": {
        "lr_trunk": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 5000},
        "lr_policy": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 5000},
        "lr_value_path": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 8000},
        "lr_baseline_path": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 8000},
        "entropy_coeff": {"start_value": 0.01, "end_value": 0.002, "total_iters": 8000},
        "policy_clip": {"start_value": 0.2, "end_value": 0.1, "total_iters": 8000},
        "max_grad_norm": {"start_value": 1.0, "end_value": 0.8, "total_iters": 8000}
      },
      "networks": {
        "MultiAgentEmbeddingNetwork": {
          "cross_attention_feature_extractor": {
            "agent_obs_size": 9,
            "num_agents": 4,
            "embed_dim": 128,
            "num_heads": 4,
            "transformer_layers": 1,
            "dropout_rate": 0.1,
            "use_agent_id": true,
            "ff_hidden_factor": 2,
            "id_num_freqs": 8,
            "conv_init_scale": 1.0,
            "linear_init_scale": 1.0,
            "fusion_type": "parallel",
            "central_processing_configs": [
              {
                "name": "height_map",
                "enabled": true,
                "type": "cnn",
                "cnn_stem_channels": [16, 32],
                "cnn_stem_kernels": [5, 3],
                "cnn_stem_strides": [2, 2],
                "cnn_stem_group_norms": [4, 8],
                "target_pool_scale": 2,
                "add_spatial_coords_to_cnn_input": true,
                "swin_stages": [
                  {"window_size": 2, "num_heads": 4, "merge": false}
                ]
              },
              {
                "name": "gridobject_sequence",
                "enabled": true,
                "type": "linear_sequence",
                "linear_stem_hidden_sizes": [64],
                "add_positional_encoding": true
              }
            ]
          },
          "obs_encoder": {
            "state_dim": 128,
            "hidden_size": 128,
            "output_size": 128,
            "dropout_rate": 0.1,
            "activation": true,
            "layer_norm": true
          },
          "obs_actions_encoder": {
            "state_dim": 128,
            "action_dim": 6,
            "hidden_size": 128,
            "output_size": 128,
            "dropout_rate": 0.1,
            "activation": true,
            "layer_norm": true
          }
        },
        "policy_network": {
          "type": "beta",
          "in_features": 128,
          "hidden_size": 128,
          "param_init_bias": 0.0,
          "min_concentration": 1.01,
          "linear_init_scale": 0.1,
          "dropout_rate": 0.1
        },
        "critic_network": {
          "baseline_head": {"in_features": 128, "hidden_size": 128, "dropout_rate": 0.0, "output_layer_init_gain": 1.0},
          "value_head": {"in_features": 128, "hidden_size": 128, "dropout_rate": 0.0, "output_layer_init_gain": 1.0},
          "value_attention": {"embed_dim": 128, "num_heads": 4, "dropout": 0.1, "self_attention": true},
          "baseline_attention": {"embed_dim": 128, "num_heads": 4, "dropout": 0.1, "self_attention": true},
          "distributional_network_params_disabled": {
            "iqn_network_disabled": {
              "feature_dim": 128,
              "hidden_size": 128,
              "quantile_embedding_dim": 64,
              "num_quantiles": 16,
              "use_dueling": true,
              "linear_init_scale": 1.0,
              "dropout_rate": 0.1,
              "iqn_kappa": 1.0
            },
            "fqf_network_disabled": {
              "feature_dim": 128,
              "hidden_size": 128,
              "quantile_embedding_dim": 64,
              "num_quantiles": 16,
              "use_dueling": true,
              "linear_init_scale": 1.0,
              "dropout_rate": 0.1,
              "iqn_kappa": 1.0,
              "fraction_net_hidden": 64,
              "fraction_loss_coeff": 0.05,
              "fraction_entropy_coeff": 0.02,
              "softmax_temperature": 3.0,
              "prior_blend_alpha": 0.2,
              "exploration_noise_scale": 0.02,
              "logit_clamp_epsilon": 1e-08,
              "monotonicity_epsilon": 1e-06
            }
          }
        }
      }
    }
  },
  "train": {
    "sequence_length": 32,
    "pad_trajectories": true,
    "batch_size": 512,
    "buffer_size": 512,
    "num_environments": 16,
    "epochs": 4,
    "mini_batch_size": 32,
    "ActionRepeat": 2,
    "saveFrequency": 19,
    "states_normalizer_disabled": {"epsilon": 1e-08, "warmup_steps": 0}
  },
  "test": {"enabled": false, "frequency": 1, "steps": 128}
}
