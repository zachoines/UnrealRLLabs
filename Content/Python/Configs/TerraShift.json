{
  "environment": {
    "name": "TerraShift_8A_2O_Test",
    "id": "TerraShift_8A_2O_Test_$(date +%Y%m%d_%H%M%S)",
    "params": {
      "PlatformSize": 1.0,
      "MaxColumnHeight": 6.0,
      "ObjectSize": [0.15, 0.15, 0.15],
      "ObjectMass": 0.1,
      "GridSize": 20,
      "MaxSteps": 128,
      "MaxAgents": 8,
      "TerraShiftEnvironment": {
        "bUseVelAlignment": false,
        "bUseXYDistanceImprovement": true,
        "bUseZAccelerationPenalty": false,
        "bUsePotentialShaping": false,
        "PotentialShaping_Gamma": 0.99,
        "PotentialShaping_Scale": 1.0,
        "VelAlign_Scale": 0.01,
        "VelAlign_Min": 0.0,
        "VelAlign_Max": 100.0,
        "DistImprove_Scale": 10.0,
        "DistImprove_Min": 0.0,
        "DistImprove_Max": 1.0,
        "ZAccel_Scale": 0.0001,
        "ZAccel_Min": 0.1,
        "ZAccel_Max": 2000.0,
        "EventReward_GoalReached": 1.0,
        "EventReward_OutOfBounds": -1.0,
        "TimeStepPenalty": 0.0
      },
      "GoalManager": {"DefaultRadius": 1.0},
      "MultiAgentGaussianWaveHeightMap": {
        "NumAgents": 8,
        "GridSize": 20,
        "MinHeight": -6.0,
        "MaxHeight": 6.0,
        "NumActions": 6,
        "bUseActionDelta": true,
        "bAccumulatedWave": false,
        "AccumulatedWaveFadeGamma": 0.95,
        "bWrapAroundEdges": true,
        "VelMinMax": [-8.0, 8.0],
        "AngVelRange": [-3.14, 3.14],
        "AmpMinMax": [-6.0, 6.0],
        "SigMinMax": [0.5, 4.0],
        "DeltaVelScale": 1.0,
        "DeltaAngVelScale": 0.3925,
        "DeltaAmpScale": 0.75,
        "DeltaSigmaScale": 0.28125
      },
      "StateManager": {
        "MaxGridObjects": 3,
        "MarginXY": 1.5,
        "MinZ": 0.0,
        "MaxZ": 40.0,
        "ObjectScale": 0.15,
        "ObjectMass": 0.1,
        "MaxColumnHeight": 6.0,
        "BaseRespawnDelay": 0.0,
        "OverheadCameraDistance_disabled": 1500.0,
        "OverheadCameraFOV_disabled": 90.0,
        "OverheadCameraResX_disabled": 25,
        "OverheadCameraResY_disabled": 25,
        "bUseRandomGoals": true,
        "bRespawnOnGoal": false,
        "bRespawnOnOOB": false,
        "bTerminateOnAllGoalsReached": true,
        "bTerminateOnMaxSteps": false,
        "GoalRadius": 7.0,
        "ObjectRadius": 7.0,
        "GoalCollectRadius": 10.0,
        "GoalColors": [
          [1.0, 1.0, 0.0],
          [1.0, 0.0, 1.0],
          [0.0, 1.0, 1.0]
        ],
        "GridObjectColors": [
          [1.0, 1.0, 0.0],
          [1.0, 0.0, 1.0],
          [0.0, 1.0, 1.0]
        ],
        "bIncludeHeightMapInState": true,
        "StateHeightMapResolutionH": 64,
        "StateHeightMapResolutionW": 64,
        "bIncludeOverheadImageInState": false,
        "StateOverheadImageResX": 60,
        "StateOverheadImageResY": 60,
        "bIncludeGridObjectSequenceInState": true,
        "MaxGridObjectsForState": 3,
        "GridObjectFeatureSize": 9
      }
    },
    "shape": {
      "state": {
        "central": [
          {
            "name": "height_map",
            "enabled": true,
            "type": "matrix2d",
            "shape": {"h": 64, "w": 64, "c": 1}
          },
          {
            "name": "greyscale_image",
            "enabled": false,
            "type": "matrix2d",
            "shape": {"h": 64, "w": 64, "c": 1}
          },
          {
            "name": "gridobject_sequence",
            "enabled": true,
            "type": "sequence",
            "shape": {"max_length": 3, "feature_dim": 9}
          }
        ],
        "agent": {"obs_size": 9, "min": 8, "max": 8}
      },
      "action": {
        "agent": {
          "continuous": [
            {"min": -1, "max": 1},
            {"min": -1, "max": 1},
            {"min": -1, "max": 1},
            {"min": -1, "max": 1},
            {"min": -1, "max": 1},
            {"min": -1, "max": 1}
          ]
        }
      }
    }
  },
  "StateRecorder": {
    "enabled": false,
    "video_settings": {"output_path": "recordings/TerraShift.mp4", "fps": 15, "dpi": 150},
    "auto_save_every": 512,
    "components_to_record": [
      {
        "enabled": true,
        "source_key": "central",
        "component_name": "height_map",
        "visualization_type": "3d_surface",
        "plot_title": "Height Map Visualization",
        "shape_for_plot": {"h": 64, "w": 64, "c": 1},
        "z_lim_3d": [-1, 1],
        "cmap_3d": "terrain"
      }
    ]
  },
  "agent": {
    "type": "MA_POCA",
    "params": {
      "enable_memory": true,
      "memory_type": "gru",
      "gru": {"input_size": 128, "hidden_size": 128, "num_layers": 2, "bias": true, "dropout": 0.1},
      "enable_iqn_distillation": false,
      "disagreement_rewards_disabled": {
        "enabled": false,
        "reward_coeff": 0.10,
        "loss_coeff": 0.5,
        "normalize_reward": true,
        "ensemble_size": 8,
        "network": {"hidden_size": 64}
      },
      "rnd_reward_disabled": {
        "enabled": false,
        "reward_coeff": 0.01,
        "loss_coeff": 1.0,
        "update_proportion": 0.25,
        "normalize_reward": false,
        "network": {"output_size": 128, "hidden_size": 256}
      },
      "enable_popart": false,
      "popart_beta": 0.995,
      "popart_epsilon": 1e-04,
      "learning_rate": 1e-04,
      "lr_policy": 1e-04,
      "lr_trunk": 1e-04,
      "lr_value_path": 1e-04,
      "lr_baseline_path": 1e-04,
      "lr_value_iqn": 1e-04,
      "lr_baseline_iqn": 1e-04,
      "lr_rnd_predictor": 1e-04,
      "lr_disagreement": 1e-04,
      "optimizer_betas": {"default": [0.9, 0.999]},
      "weight_decay": {"default": 0},
      "gamma": 0.99,
      "lmbda": 0.95,
      "value_loss_coeff": 0.5,
      "baseline_loss_coeff": 0.25,
      "normalize_advantages": true,
      "max_grad_norm": 0.5,
      "ppo_clip_range": 0.2,
      "value_clip_range": 0.1,
      "entropy_coeff": 0.01,
      "schedulers": {
        "lr_trunk": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "lr_policy": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "lr_value_path": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "lr_baseline_path": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "lr_value_iqn": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "lr_baseline_iqn": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "lr_rnd_predictor": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "lr_disagreement": {"start_factor": 1.0, "end_factor": 0.1, "total_iters": 50000},
        "entropy_coeff": {"start_value": 0.005, "end_value": 0.001, "total_iters": 50000},
        "policy_clip": {"start_value": 0.2, "end_value": 0.1, "total_iters": 50000},
        "value_clip": {"start_value": 0.01, "end_value": 0.001, "total_iters": 50000},
        "max_grad_norm": {"start_value": 0.5, "end_value": 0.5, "total_iters": 50000}
      },
      "networks": {
        "MultiAgentEmbeddingNetwork": {
          "cross_attention_feature_extractor": {
            "agent_obs_size": 9,
            "num_agents": 8,
            "embed_dim": 128,
            "num_heads": 8,
            "transformer_layers": 2,
            "dropout_rate": 0.1,
            "use_agent_id": true,
            "ff_hidden_factor": 2,
            "id_num_freqs": 8,
            "conv_init_scale": 1.0,
            "linear_init_scale": 1.0,
            "central_processing_configs": [
              {
                "name": "height_map",
                "enabled": true,
                "type": "cnn",
                "cnn_stem_channels": [32, 64, 64],
                "cnn_stem_kernels": [8, 4, 3],
                "cnn_stem_strides": [4, 2, 1],
                "cnn_stem_group_norms": [8, 8, 8],
                "target_pool_scale": 8,
                "add_spatial_coords_to_cnn_input": true,
                "swin_stages": [
                  {"window_size": 4, "merge": true}
                ],
                "final_patch_tokens": 16
              },
              {
                "name": "gridobject_sequence",
                "enabled": true,
                "type": "linear_sequence",
                "linear_stem_hidden_sizes": [128, 128],
                "add_positional_encoding": true
              }
            ]
          },
          "obs_encoder": {
            "state_dim": 128,
            "hidden_size": 128,
            "output_size": 128,
            "dropout_rate": 0.1,
            "activation": true,
            "layer_norm": true
          },
          "obs_actions_encoder": {
            "state_dim": 128,
            "action_dim": 6,
            "hidden_size": 128,
            "output_size": 128,
            "dropout_rate": 0.1,
            "activation": true,
            "layer_norm": true
          }
        },
        "policy_network": {
          "type": "beta",
          "in_features": 128,
          "hidden_size": 128,
          "param_init_bias": 0.0,
          "min_concentration": 1.001,
          "linear_init_scale": 0.1,
          "dropout_rate": 0.1
        },
        "critic_network": {
          "feature_dim_for_iqn": 128,
          "value_head": {
            "in_features": 128,
            "hidden_size": 128,
            "dropout_rate": 0.0,
            "output_layer_init_gain": -3.5
          },
          "baseline_head": {
            "in_features": 128,
            "hidden_size": 128,
            "dropout_rate": 0.0,
            "output_layer_init_gain": -3.5
          },
          "value_attention": {
            "embed_dim": 128,
            "num_heads": 8,
            "dropout": 0.1,
            "self_attention": true
          },
          "baseline_attention": {
            "embed_dim": 128,
            "num_heads": 8,
            "dropout": 0.1,
            "self_attention": true
          },
          "iqn_params": {
            "num_quantiles": 32,
            "num_quantiles_prime": 32,
            "kappa": 1.0,
            "iqn_polyak": 0.005,
            "iqn_epochs": 1,
            "iqn_max_grad_norm": 0.5,
            "value_iqn_loss_coeff": 0.5,
            "baseline_iqn_loss_coeff": 0.25,
            "iqn_target_update_freq": 1,
            "iqn_batch_size": 64,
            "iqn_replay_buffer_size": 50000,
            "iqn_network": {
              "feature_dim": 128,
              "hidden_size": 128,
              "quantile_embedding_dim": 128,
              "linear_init_scale": 1.0,
              "num_quantiles": 32,
              "dropout_rate": 0.0
            },
            "distillation_network": {
              "num_quantiles": 32,
              "hidden_size": 128,
              "combination_method": "hadamard_product",
              "linear_init_scale": 1.0,
              "dropout_rate": 0.0
            }
          }
        }
      }
    }
  },
  "train": {
    "sequence_length": 8,
    "pad_trajectories": true,
    "batch_size": 256,
    "buffer_size": 256,
    "num_environments": 8,
    "epochs": 4,
    "mini_batch_size": 32,
    "ActionRepeat": 1,
    "saveFrequency": 50,
    "states_normalizer_disabled": {"epsilon": 1e-08, "warmup_steps": 0}
  }
}
